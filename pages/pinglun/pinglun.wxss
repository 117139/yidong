/* pages/pinglun/pinglun.wxss */
.contbox{
	width: 100%;
  min-height: 100vh;
	padding: 20rpx 0;
	box-sizing: border-box;
  background: #eee;
}
.imgbox {
	margin-top: 20rpx;
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}
.imgbox image{
	width: 190rpx;
	height: 190rpx;
	margin-bottom: 10rpx;
	border-radius: 30rpx;
}
.imgbox .One{
	width: 300rpx;
	height: 300rpx;
}